Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"toCWPair", "[", "i_Integer", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"b", "=", 
        RowBox[{"Rest", "@", 
         RowBox[{"IntegerDigits", "[", 
          RowBox[{"i", ",", "2"}], "]"}]}]}], ",", 
       RowBox[{"t", "=", 
        RowBox[{"{", 
         RowBox[{"1", ",", "1"}], "}"}]}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Scan", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"#", "==", "0"}], ",", 
           RowBox[{"t", "=", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"First", "[", "t", "]"}], ",", 
              RowBox[{"Plus", "@@", "t"}]}], "}"}]}], ",", 
           RowBox[{"t", "=", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"Plus", "@@", "t"}], ",", 
              RowBox[{"Last", "[", "t", "]"}]}], "}"}]}]}], "]"}], "&"}], ",",
         "b"}], "]"}], ";", "\[IndentingNewLine]", "t"}]}], "]"}]}], 
  ";"}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.8764763157302823`*^9, 3.876476329443552*^9}, {
   3.876476424455309*^9, 3.876476469317542*^9}, 3.8764773958109713`*^9, 
