Cell[BoxData[
 RowBox[{
  RowBox[{"cnn", "[", "n_", "]"}], ":=", 
  RowBox[{
   RowBox[{"3", " ", "n"}], "-", 
   RowBox[{
    FractionBox["1", "2"], 
    RowBox[{"(", 
     RowBox[{"1", "-", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], "n"]}], ")"}]}]}]}]], "Input",
 InitializationCell->True,
