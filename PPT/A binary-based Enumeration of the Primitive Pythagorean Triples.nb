         RowBox[{"1", ",", "1"}], "}"}]}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Scan", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"#", "==", "0"}], ",", 
           RowBox[{"t", "=", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"First", "[", "t", "]"}], ",", 
              RowBox[{"Plus", "@@", "t"}]}], "}"}]}], ",", 
           RowBox[{"t", "=", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"Plus", "@@", "t"}], ",", 
              RowBox[{"Last", "[", "t", "]"}]}], "}"}]}]}], "]"}], "&"}], ",",
