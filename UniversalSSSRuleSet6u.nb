877aefeaf624"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "SSSNewRule", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"SSSNewRule", "[", 
    RowBox[{"rulenum_Integer", ",", 
     RowBox[{"(", 
      RowBox[{"rule_Rule", " ", "|", " ", "rule_RuleDelayed"}], ")"}]}], 
    "]"}], " ", ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "lhs", ",", "rhs", ",", "lhsNames", ",", "newlhs", ",", "newrhs1", ",", 
       "newrhs2"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{"lhs", ",", "rhs"}], "}"}], "=", 
       RowBox[{"List", "@@", "rule"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"lhsNames", " ", "=", " ", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Unique", "[", "lhsTag", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"StringLength", "[", "lhs", "]"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"newlhs", "=", 
       RowBox[{"ToString", "[", 
        RowBox[{"s", "@@", 
         RowBox[{"Transpose", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"fromAlpha", "[", "lhs", "]"}], ",", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"ToString", "@", "#"}], "<>", "\"\<_\>\""}], "&"}], 
             " ", "/@", " ", "lhsNames"}]}], "}"}], "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"newrhs1", "=", 
       RowBox[{"(", 
        RowBox[{"\"\<AppendTo[$SSSConnectionList, \>\"", "<>", 
         RowBox[{"ToString", "[", "lhsNames", "]"}], "<>", 
         "\"\< \[Rule] $SSSTagIndex + \>\"", "<>", 
         RowBox[{"ToString", "[", 
          RowBox[{
           RowBox[{"Range", "@", 
            RowBox[{"StringLength", "@", "rhs"}]}], "-", "1"}], "]"}], "<>", 
         "\"\<]; \>\""}], ")"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"newrhs2", "=", 
       RowBox[{"ToString", "[", 
        RowBox[{
         RowBox[{"SSSConvert", "[", "rhs", "]"}], " ", "/.", " ", 
         RowBox[{"n_Integer", " ", "\[RuleDelayed]", " ", 
          RowBox[{"{", 
           RowBox[{"n", ",", "\"\<$SSSTagIndex++\>\""}], "}"}]}]}], "]"}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"ToExpression", "[", 
       RowBox[{
       "newlhs", "<>", "\"\< \[RuleDelayed] (\>\"", "<>", 
        "\"\<AppendTo[$SSSRulesUsed,\>\"", "<>", 
