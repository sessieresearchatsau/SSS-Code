   3.906565361654678*^9}, {3.906565404971857*^9, 3.9065654586558986`*^9}, {
   3.9066468393079333`*^9, 3.906646858682765*^9}, {3.906646908437312*^9, 
   3.906646909244627*^9}, {3.907173105277307*^9, 3.907173120060948*^9}, {
